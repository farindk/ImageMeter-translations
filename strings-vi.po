#. errors
msgctxt "imageselect_error_imported_file_is_not_an_image"
msgid "Imported file is not an image"
msgstr "Tệp đã nhập không phải là hình ảnh"

msgctxt "imageselect_error_cannot_edit_corrupted_image"
msgid "The .imm file for this image is corrupted. You cannot edit the image."
msgstr "Tệp .imm cho hình ảnh này bị hỏng. Bạn không thể chỉnh sửa hình ảnh."

msgctxt "imageselect_error_cannot_rename_corrupted_image"
msgid "The .imm file for this image is corrupted. You cannot rename the image."
msgstr "Tệp .imm cho hình ảnh này bị hỏng. Bạn không thể đổi tên hình ảnh."

msgctxt "imageselect_error_cannot_export_corrupted_image"
msgid "The images with the corrupted .imm files will not be exported."
msgstr "Các hình ảnh với các tệp .imm bị hỏng sẽ không được xuất."

#, c-format
msgctxt "imageselect_error_cannot_edit_missing_image"
msgid ""
"The image file is missing. Unfortunately, you cannot edit the image without.\n"
"\n"
"Expected image filename: %1$s."
msgstr "Các tập tin hình ảnh bị thiếu. Thật không may, bạn không thể chỉnh sửa hình ảnh mà không có. \\ N \\ n Tên tệp hình ảnh được mở rộng: %1$s ."

#, c-format
msgctxt "imageselect_error_image_version_more_recent_open_editor_anyways"
msgid ""
"The file version of this image (%1$d) is more recent than the maximum version supported by the ImageMeter version installed (%2$d). Opening the file may result in lost data or wrong annotations. You should use a recent version of ImageMeter to open this file.\n"
"\n"
"Do you still want to open it?"
msgstr "Phiên bản tệp của hình ảnh này ( %1$d ) gần đây hơn phiên bản tối đa được hỗ trợ bởi phiên bản ImageMeter được cài đặt ( %2$d ). Mở tệp có thể dẫn đến mất dữ liệu hoặc chú thích sai. Bạn nên sử dụng phiên bản ImageMeter gần đây để mở tệp này. \\ N \\ nBạn có muốn mở nó không?"

msgctxt "imageselect_error_metadata_format_unknown"
msgid "Image metadata file (.imm) cannot be read, because it's format is unknown."
msgstr "Không thể đọc tệp siêu dữ liệu hình ảnh (.imm) vì không xác định được định dạng của nó."

msgctxt "imageselect_error_cannot_modify_readonly_folder"
msgid "This is an essential system folder, you cannot remove or rename it."
msgstr "Đây là một thư mục hệ thống thiết yếu, bạn không thể loại bỏ hoặc đổi tên nó."

msgctxt "imageselect_error_cannot_move_bundle_to_folder"
msgid "Cannot move image to new folder (maybe an image with the same name already exists)."
msgstr "Không thể di chuyển hình ảnh sang thư mục mới (có thể một hình ảnh có cùng tên đã tồn tại)."

msgctxt "imageselect_error_external_memory_unavailable"
msgid "External memory (where your app-data is stored) is not available. Most probably, your device stores this data on an external SD card which is currently not available. If this is not the problem in your case, you may contact imagemeter@gmail.com."
msgstr "Bộ nhớ ngoài (nơi lưu trữ dữ liệu ứng dụng của bạn) không khả dụng. Rất có thể, thiết bị của bạn lưu trữ dữ liệu này trên thẻ SD bên ngoài hiện không khả dụng. Nếu đây không phải là vấn đề trong trường hợp của bạn, bạn có thể liên hệ với fantemeter @ gmail."

#, c-format
msgctxt "imageselect_error_folder_cannot_create"
msgid "Cannot create the folder '%1$s' within this directory: '%2$s'. Make sure the media (e.g. SD-card) is mounted, this directory exists and is writable."
msgstr "Không thể tạo thư mục ' %1$s ' trong thư mục này: ' %2$s '. Đảm bảo phương tiện (ví dụ thẻ SD) được gắn, thư mục này tồn tại và có thể ghi."

#, c-format
msgctxt "imageselect_error_cannot_create_databundle_on_filesystem"
msgid "Cannot create filesystem folder '%1$s' for the image data. Make sure the media (e.g. SD-card) is mounted, this directory exists and is writable."
msgstr "Không thể tạo thư mục hệ thống tập tin ' %1$s ' cho dữ liệu hình ảnh. Đảm bảo phương tiện (ví dụ thẻ SD) được gắn, thư mục này tồn tại và có thể ghi."

#, c-format
msgctxt "imageselect_error_cannot_access_external_data"
msgid "Cannot access external data at location '%1$s'."
msgstr "Không thể truy cập dữ liệu ngoài tại vị trí ' %1$s '."

#, c-format
msgctxt "imageselect_error_cannot_create_existing_folder"
msgid "Cannot create folder '%1$s' because it already exists."
msgstr "Không thể tạo thư mục ' %1$s ' vì nó đã tồn tại."

#, c-format
msgctxt "imageselect_error_folder_rename_target_exists"
msgid "Cannot rename folder to '%1$s' because that already exists."
msgstr "Không thể đổi tên thư mục thành ' %1$s ' vì điều đó đã tồn tại."

#, c-format
msgctxt "imageselect_error_folder_does_not_exist"
msgid "Folder '%1$s' does not exist anymore."
msgstr "Thư mục ' %1$s ' không còn tồn tại nữa."

#, c-format
msgctxt "imageselect_error_folder_does_not_exist_detailed"
msgid "Folder '%1$s' does not exist anymore (expected location '%2$s')."
msgstr "Thư mục ' %1$s ' không còn tồn tại nữa (vị trí dự kiến ' %2$s ')."

#, c-format
msgctxt "imageselect_error_folder_cannot_delete"
msgid "Cannot delete folder '%1$s'."
msgstr "Không thể xóa thư mục ' %1$s '."

#, c-format
