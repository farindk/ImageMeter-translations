msgctxt "UNUSED_imageselect_error_cannot_rename_corrupted_image"
msgid "The .imm file for this image is corrupted. You cannot rename the image."
msgstr "Die Daten dieses Bildes sind beschädigt, es kann nicht umbenannt werden."

msgctxt "UNUSED_imageselect_error_cannot_export_corrupted_image"
msgid "The images with the corrupted .imm files will not be exported."
msgstr "Die Bilder mit den beschädigten Daten werden nicht exportiert."

#, c-format
msgctxt "UNUSED_imageselect_error_image_version_more_recent_open_editor_anyways"
msgid ""
"The file version of this image (%1$d) is more recent than the maximum version "
"supported by the ImageMeter version installed (%2$d). Opening the file may result in "
"lost data or wrong annotations. You should use a recent version of ImageMeter to open "
"this file.\n"
"\n"
"Do you still want to open it?"
msgstr ""
"Die Dateiversion dieses Bildes (%1$d) ist neuer als die maximale Version, die von der "
"installierten Version von ImageMeter unterstützt wird (%2$d). Wird diese Datei "
"geöffnet, können dadurch Daten verloren gehen oder fehlerhaft angezeigt werden. Sie "
"sollten eine aktuellere Version von ImageMeter verwenden, um dieses Bild zu öffnen.\n"
"\n"
"Wollen Sie das Bild trotzdem öffnen?"

msgctxt "UNUSED_imageselect_error_metadata_format_unknown"
msgid "Image metadata file (.imm) cannot be read, because it's format is unknown."
msgstr ""
"Bildannotation (.imm-Datei) kann nicht gelesen werden, da das Format unbekannt ist."

msgctxt "UNUSED_imageselect_error_cannot_modify_readonly_folder"
msgid "This is an essential system folder, you cannot remove or rename it."
msgstr ""
"Dies ist ein wichtiger Systemordner, der nicht gelöscht oder umbenannt werden kann."

msgctxt "UNUSED_imageselect_error_cannot_move_bundle_to_folder"
msgid ""
"Cannot move image to new folder (maybe an image with the same name already exists)."
msgstr ""
"Kann das Bild nicht in den Ordner verschieben (evtl. existiert dort schon ein Bild "
"mit gleichem Namen)."

msgctxt "UNUSED_imageselect_error_external_memory_unavailable"
msgid ""
"External memory (where your app-data is stored) is not available. Most probably, your "
"device stores this data on an external SD card which is currently not available. If "
"this is not the problem in your case, you may contact info@imagemeter.com."
msgstr ""
"Externer Speicher (in dem die App-Daten abgelegt werden) ist nicht verfügbar. "
"Wahrscheinlich speichert Ihr Gerät diese Daten auf einer externen SD-Karte, welche im "
"Moment aber nicht eingelegt ist. Sollte das nicht das Problem sein, können Sie sich "
"an info@imagemeter.com wenden."

#, c-format
msgctxt "UNUSED_imageselect_error_folder_cannot_create"
msgid ""
"Cannot create the folder '%1$s' within this directory: '%2$s'. Make sure the media (e."
"g. SD-card) is mounted, this directory exists and is writable."
msgstr ""
"Kann das Verzeichnis '%1$s' nicht in dem Verzeichnis '%2$s' erstellen. Stellen Sie "
"sicher, dass evtl. die Speicherkarte eingelegt ist, dieses Verzeichnis existiert und "
"beschreibbar ist."

#, c-format
msgctxt "UNUSED_imageselect_error_cannot_access_external_data"
msgid "Cannot access external data at location '%1$s'."
msgstr "Kann die folgende externe Datei nicht öffnen: '%1$s'."

#, c-format
msgctxt "UNUSED_imageselect_error_cannot_create_existing_folder"
msgid "Cannot create folder '%1$s' because it already exists."
msgstr "Kann Verzeichnis '%1$s' nicht erstellen, da es schon existiert."

#, c-format
msgctxt "UNUSED_imageselect_error_folder_rename_target_exists"
msgid "Cannot rename folder to '%1$s' because that already exists."
msgstr "Kann Verzeichnis nicht nach '%1$s' umbenennen, da es schon existiert."

#, c-format
msgctxt "UNUSED_imageselect_error_folder_does_not_exist_detailed"
msgid "Folder '%1$s' does not exist anymore (expected location '%2$s')."
msgstr "Verzeichnis '%1$s' existiert nicht (erwarteter Ort: '%2$s')."

#, c-format
msgctxt "UNUSED_imageselect_error_cannot_write_data_bundle"
msgid "Cannot write data for image '%1$s'."
msgstr "Kann die Daten für Bild '%1$s' nicht schreiben."

#, c-format
msgctxt "UNUSED_imageselect_error_cannot_write_data_bundle_detailed"
msgid "Cannot write data for image '%1$s' (at location '%2$s')."
msgstr "Kann die Daten für Bild '%1$s' nicht schreiben (Ort: '%2$s')."

#, c-format
msgctxt "UNUSED_imageselect_error_cannot_delete_data_bundle"
msgid "Cannot delete image '%1$s'."
msgstr "Kann Bild '%1$s' nicht löschen."

msgctxt "UNUSED_imageselect_error_url_of_imported_file_not_found"
msgid "Image to be imported cannot be found."
msgstr "Das zu importierende Bild kann nicht gefunden werden."

#, c-format
msgctxt "UNUSED_error_cannot_save_annotated_image"
msgid "Cannot save annotated image because: '%1$s'."
msgstr "Kann das bearbeitete Bild nicht speichern, da: '%1$s'."

#, c-format
msgctxt "UNUSED_error_cannot_save_thumbnail_image"
msgid "Cannot save thumbnail image because: '%1$s'."
msgstr "Kann das Vorschaubild nicht speichern, da: '%1$s'."

#, c-format
msgctxt "UNUSED_error_cannot_access_files_in_folder"
msgid "Cannot access files in folder '%1$s'."
msgstr "Auf die Dateien im Verzeichnis '%1$s' kann nicht zugegriffen werden."

#, c-format
msgctxt "UNUSED_error_target_folder_contains_bundle_with_same_id"
msgid "Folder '%1$s' contains image '%2$s' with the same ID as image '%3$s'."
msgstr ""
"Verzeichnis '%1$s' enthält das Bild '%2$s', welches die gleiche ID wie das Bild "
"'%3$s' hat."

msgctxt "UNUSED_storage_error_external_storage_read_only"
msgid "External storage is read-only."
msgstr "Externer Speicher ist nur lesbar."

msgctxt "UNUSED_storage_error_external_storage_could_not_write_file"
msgid "Could not write file to external memory."
msgstr "Konnte die Datei nicht in externen Speicher abspeichern."

msgctxt "UNUSED_storage_error_external_storage_could_not_get_permission_to_write_images"
msgid ""
"Could not get permission to write to external storage. No files have been written."
msgstr ""
"Konnte die Berechtigung auf den externen Speicher zu schreiben nicht erhalten. Bilder "
"wurden nicht gespeichert."

#, c-format
msgctxt "UNUSED_storage_error_cannot_create_directory_on_external_storage"
msgid "Could not create directory '%1$s' on external storage."
msgstr "Konnte das Verzeichnis '%1$s' nicht im externen Speicher erstellen."

msgctxt "UNUSED_storage_error_external_storage_destination_is_no_directory"
msgid "External storage destination is not a directory."
msgstr "Externer Speicherort ist kein Verzeichnis."

#, c-format
msgctxt "UNUSED_storage_error_data_bundle_corrupted"
msgid "Data in image directory '%1$s' is corrupted."
msgstr "Daten im Bildverzeichnis '%1$s' sind beschädigt."

msgctxt "UNUSED_storage_error_no_permission_to_read_image_to_be_imported"
msgid ""
"Cannot import image because the permission to read external storage has not been "
"granted."
msgstr ""
"Kann das Bild nicht importieren, da die Leseberechtigung dazu nicht erteilt wurde."

#, c-format
msgctxt "UNUSED_storage_error_cannot_read_ifd_file"
msgid "Cannot read IFD file in folder '%1$s'."
msgstr "Die IFD-Datei im Verzeichnis '%1$s' kann nicht gelesen werden."

#, c-format
msgctxt "UNUSED_error_cannot_open_file_does_not_exist"
msgid "Cannot open file '%1$s' because it does not exist."
msgstr "Die Datei '%1$s' kann nicht geöffnet werden, da sie nicht existiert."

#, c-format
msgctxt "UNUSED_error_cannot_open_file_is_folder"
msgid "Cannot open file '%1$s' because it is a folder."
msgstr "Die Datei '%1$s' kann nicht gelesen werden, da es ein Verzeichnis ist."

#, c-format
msgctxt "UNUSED_error_cannot_write_file"
msgid "Cannot write to file '%1$s'."
msgstr "Kann die Datei '%1$s' nicht schreiben."

msgctxt "UNUSED_storage_error_cannot_load_image_file_from_remote"
msgid "Cannot load the image file from cloud storage."
msgstr "Die Bilddatei kann nicht vom Cloudspeicher gelesen werden."

msgctxt "UNUSED_storage_note_pulling_image_file_from_remote"
msgid "Downloading image file from cloud storage."
msgstr "Bilddatei wird vom Cloudspeicher runtergeladen."

