msgid "Resetting cloud server cache"
msgstr "Скидання кешу хмарного сервера"

msgctxt "sync:server-maintenance:remove-server-cache:result:success"
msgid "Successfully resetted server cache."
msgstr "Кеш сервера успішно скинуто."

